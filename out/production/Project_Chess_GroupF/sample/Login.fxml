<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <TextField fx:id="portTextField" layoutX="114.0" layoutY="44.0" />
      <TextField fx:id="ipTextField" layoutX="114.0" layoutY="89.0" />
      <Label layoutX="37.0" layoutY="48.0" text="Port" />
      <Label layoutX="37.0" layoutY="93.0" text="IP-address" />
      <Button fx:id="submitButton" layoutX="124.0" layoutY="140.0" mnemonicParsing="false" onAction="#onClickSubmit" text="Submit" />
      <Text fx:id="errorMessage" fill="RED" layoutX="44.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="213.0" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <TextField fx:id="usernameTextField" layoutX="101.0" layoutY="64.0" promptText="Enter username" />
      <Text fx:id="errorMessage" fill="RED" layoutX="68.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="213.0" />
      <Button fx:id="createGameButton" layoutX="65.0" layoutY="135.0" mnemonicParsing="false" onAction="#onClickCreate" prefHeight="50.0" prefWidth="100.0" text="Create a game" />
      <Button fx:id="joinGameButton" layoutX="185.0" layoutY="135.0" mnemonicParsing="false" onAction="#onClickJoin" prefHeight="50.0" prefWidth="100.0" text="Join a game" />
      <Button fx:id="submitButton" layoutX="148.0" layoutY="201.0" mnemonicParsing="false" onAction="#onClickSubmit" text="Submit" />
   </children>
</AnchorPane>
